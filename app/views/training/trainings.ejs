<% for( let i = 0; i < trainings.length; i++ ) { %>
    <div class="exercise-box trainingList">
            <a class="trainingLink" href="/training/<%= encodeURIComponent(trainings[i].name) %> "><%= trainings[i].name %></a>
            <small class="datetime">Dodano: <%= trainings[i].added_dttm.toLocaleString('pl-PL', {
                weekday: 'long',
                day: 'numeric', 
                year: 'numeric', 
                month: 'long',
                hour: 'numeric',
                minute: 'numeric',
            } ) %></small>
    </div>

<% } %>

